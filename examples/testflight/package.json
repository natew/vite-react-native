{
  "name": "example-testflight",
  "version": "1.1.371",
  "private": true,
  "type": "module",
  "scripts": {
    "android": "EXPO_NO_TELEMETRY=true one run:android",
    "build:web": "one build",
    "clean": "one clean",
    "db:generate": "drizzle-kit generate --config ./code/db/drizzle.config.ts",
    "db:init": "$npm_execpath db:generate && $npm_execpath db:push && $npm_execpath db:seed",
    "db:migrate": "dotenvx run -f .env -- tsx ./code/db/run-migrations.ts",
    "db:push": "drizzle-kit push --config ./code/db/drizzle.config.ts",
    "db:seed": "dotenvx run -f .env -- tsx ./code/db/seed.ts",
    "dev": "one dev",
    "dev:clean": "one dev --clean",
    "ios": "EXPO_NO_TELEMETRY=true one run:ios",
    "prebuild:native": "one prebuild",
    "serve": "one serve",
    "upgrade:tamagui": "$npm_execpath up '*tamagui*' '@tamagui/*'"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.12.1",
    "@tamagui/config": "^1.120.2",
    "@tamagui/image-next": "^1.120.2",
    "@tamagui/lucide-icons": "^1.120.2",
    "@vxrn/color-scheme": "workspace:^",
    "better-sqlite3": "^11.2.1",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.24.2",
    "drizzle-orm": "^0.33.0",
    "expo": "~52.0.17",
    "expo-modules-core": "2.1.2",
    "one": "workspace:^",
    "postgres": "^3.4.4",
    "react": "^18.3.1",
    "react-native": "0.76.4",
    "react-native-reanimated": "~3.16.5",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "4.4.0-rc.1",
    "react-native-svg": "15.8.0",
    "react-native-web": "^0.19.12",
    "tamagui": "^1.120.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@faker-js/faker": "next",
    "@tamagui/vite-plugin": "^1.120.2",
    "tsx": "^4.19.0",
    "vite": "^6.0.5"
  },
  "installConfig": {
    "hoistingLimits": "workspaces"
  }
}
