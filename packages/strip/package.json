{
  "name": "@vxrn/strip",
  "version": "1.1.416",
  "license": "BSD-3-Clause",
  "sideEffects": false,
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/esm/index.mjs"
    }
  },
  "module": "dist",
  "source": "src/index.ts",
  "types": "./src/index.ts",
  "bin": "run.mjs",
  "files": [
    "src",
    "types",
    "dist"
  ],
  "scripts": {
    "strip-chat": "dotenv -- node ./run.mjs ../../apps/chat ../../tmp/chat --overwrite",
    "start": "dotenv -- node ./run.mjs",
    "build": "tamagui-build --skip-types --skip-native",
    "watch": "yarn build --watch",
    "clean": "tamagui-build clean",
    "clean:build": "tamagui-build clean:build",
    "lint:fix": "../../node_modules/.bin/biome check --write --unsafe src",
    "test": "vitest",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.1.5",
    "@ai-sdk/deepseek": "^0.1.6",
    "@ai-sdk/google": "^1.0.12",
    "@ai-sdk/openai": "^1.1.5",
    "@ai-sdk/xai": "^1.1.6",
    "ai": "^4.0.22",
    "citty": "^0.1.6",
    "dotenv": "^16.4.5",
    "fs-extra": "^11.2.0",
    "typescript": "^5.7.3"
  },
  "devDependencies": {
    "@tamagui/build": "^1.123.2",
    "depcheck": "^1.4.7",
    "vitest": "^2.1.8"
  },
  "publishConfig": {
    "access": "public"
  }
}
