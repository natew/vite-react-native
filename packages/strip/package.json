{
  "name": "@vxrn/strip",
  "version": "1.1.379",
  "license": "BSD-3-Clause",
  "sideEffects": false,
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/esm/index.mjs"
    }
  },
  "module": "dist",
  "source": "src/index.ts",
  "types": "./types/index.d.ts",
  "bin": "run.mjs",
  "files": [
    "src",
    "types",
    "dist"
  ],
  "scripts": {
    "strip-chat": "dotenv -- node --max-old-space-size=24576 ./run.mjs ../../apps/chat /tmp/chat --overwrite",
    "build": "tamagui-build --skip-native",
    "watch": "yarn build --watch",
    "check": "yarn depcheck",
    "clean": "tamagui-build clean",
    "clean:build": "tamagui-build clean:build",
    "lint:fix": "../../node_modules/.bin/biome check --apply-unsafe src",
    "test": "vitest --run --pool=forks",
    "test:dev": "TEST_ONLY=dev yarn test",
    "test:watch": "vitest --watch"
  },
  "dependencies": {
    "@ai-sdk/google": "^1.0.12",
    "ai": "^4.0.22",
    "citty": "^0.1.6",
    "dotenv": "^16.4.5",
    "fs-extra": "^11.2.0"
  },
  "devDependencies": {
    "@tamagui/build": "^1.121.3",
    "vitest": "^2.1.8"
  },
  "publishConfig": {
    "access": "public"
  }
}
