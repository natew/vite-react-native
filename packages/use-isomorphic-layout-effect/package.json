  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js"
    }
  },
  "dependencies": {
    "react": "^18.3.1"
  },
  "devDependencies": {
    "@tamagui/build": "^1.121.3"
  },
  "publishConfig": {
    "access": "public"
  }
