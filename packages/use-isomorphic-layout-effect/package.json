{
  "name": "@vxrn/use-isomorphic-layout-effect",
  "version": "1.1.383",
  "source": "src/index.ts",
  "types": "./types/index.d.ts",
  "type": "module",
  "main": "dist/cjs",
  "module": "dist/esm",
  "sideEffects": false,
  "files": [
    "src",
    "types",
    "dist",
    "vendor"
  ],
  "scripts": {
    "build": "tamagui-build",
    "watch": "tamagui-build --watch",
    "lint:fix": "../../node_modules/.bin/biome check --apply-unsafe src",
