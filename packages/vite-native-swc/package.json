    "@babel/plugin-transform-regenerator": "^7.25.9",
    "@swc/core": "^1.10.4",
    "@vxrn/vite-native-client": "workspace:*",
    "metro-react-native-babel-preset": "^0.77.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@tamagui/build": "^1.121.6",
    "react-native": "^0.76.5"
  },
  "peerDependencies": {
    "react-native": "*"
  },
  "publishConfig": {
    "access": "public"
