{
  "name": "onechat",
  "version": "1.1.412",
  "private": true,
  "sideEffects": false,
  "files": [],
  "type": "module",
  "scripts": {
    "android": "expo run:android",
    "backend": "docker compose up pgdb search",
    "backend:clean": "docker compose down --volumes; rm /tmp/onechat_replica.db || true",
    "backend:run-clean": "yarn backend:clean; yarn backend",
    "build:icon": "tauri icon ./src-tauri/icons/icon.png",
    "build:tauri": "tauri build --debug",
    "build:web": "one build",
    "dev": "one dev",
    "dev:android": "one run:android",
    "dev:clean": "one dev --clean",
    "dev:ios": "one run:ios",
    "dev:tauri": "tauri dev",
    "docker": "docker compose --env-file .env",
    "ios": "expo run:ios",
    "migrate:auth": "npx @better-auth/cli migrate --config ./src/better-auth/auth.ts",
    "seed": "tsx ./src/postgres/seed.ts",
    "serve": "one serve",
    "zero": "yarn zero:build-schema && zero-cache --schema-file ./src/zero/zero-schema.json",
    "zero:build-schema": "zero-build-schema -p ./src/zero/schema-build.ts -o ./src/zero/zero-schema.json",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate --config=./drizzle.config.ts",
    "db:drop": "drizzle-kit drop --schema=./src/zero/tables.ts",
    "db:seed": "docker compose up seeder"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.709.0",
    "@aws-sdk/lib-storage": "^3.709.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@docsearch/react": "^3.6.1",
    "@floating-ui/react": "0.26.24",
    "@rocicorp/zero": "0.11.2025011402",
    "@shopify/react-native-skia": "^1.7.6",
    "@tamagui/animations-moti": "^1.123.1",
    "@tamagui/config": "^1.123.1",
    "@tamagui/helpers-icon": "^1.123.1",
    "@tamagui/linear-gradient": "^1.123.1",
    "@tamagui/lucide-icons": "^1.123.1",
    "@tamagui/proxy-tabbable": "^1.123.1",
    "@tamagui/themes": "^1.123.1",
    "@tamagui/toast": "^1.123.1",
    "@tanstack/react-table": "^8.20.6",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-deep-link": "^2.0.1",
    "@tauri-apps/plugin-fs": "^2.2.0",
    "@tauri-apps/plugin-shell": "^2",
    "@uiw/react-md-editor": "^4.0.5",
    "@vxrn/color-scheme": "workspace:*",
    "@vxrn/emitter": "workspace:*",
    "better-auth": "1.0.23-beta.3",
    "burnt": "^0.12.2",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.38.4",
    "drizzle-zero": "0.2.4",
    "expo": "~52.0.17",
    "flexsearch": "^0.7.43",
    "one": "workspace:*",
    "pg": "^8.11.0",
    "react": "^19.0.0",
    "react-hotkeys-hook": "^4.6.1",
    "react-native": "^0.76.5",
    "react-native-reanimated": "~3.16.5",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "4.4.0",
    "react-native-svg": "15.8.0",
    "sst": "3.4.46",
    "tamagui": "^1.123.1",
    "ts-deepmerge": "^7.0.2",
    "valtio": "^2.1.2",
    "virtua": "^0.39.3"
  },
  "devDependencies": {
    "@react-native-community/cli": "15.1.3",
    "@tamagui/vite-plugin": "^1.123.1",
    "@tauri-apps/cli": "^2",
    "@types/aws-lambda": "8.10.146",
    "@types/pg": "^8.10.2",
    "drizzle-kit": "^0.30.2",
    "sst": "^3.4.46",
    "tsx": "^4.19.0",
    "unicode-emoji": "^2.6.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
