{
  "name": "onechat",
  "version": "1.1.340",
  "private": true,
  "sideEffects": false,
  "files": [],
  "type": "module",
  "scripts": {
    "dev:tauri": "tauri dev",
    "build:tauri": "tauri build --debug",
    "auth:migrate": "npx @better-auth/cli migrate --config ./config/better-auth/auth.ts",
    "zero": "yarn zero:build-schema && zero-cache --schema-file ./config/zero/zero-schema.json",
    "zero:build-schema": "cd ./config/zero && zero-build-schema -p './schema.ts'",
    "docker:start:clean": "yarn data:reset; yarn docker up",
    "docker": "docker compose --env-file .env",
    "data:reset": "docker compose down; docker volume rm onechat_pgdata; rm /tmp/onechat_replica.db",
    "dev": "one dev --port 1421",
    "dev:clean": "one dev --clean",
    "subset-font-pn": "tsx ./helpers/subset-font.ts",
    "build:web": "one build",
    "serve": "one serve",
    "serve:railway": "one serve --host 0.0.0.0"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@docsearch/react": "^3.6.1",
    "@leeoniya/ufuzzy": "^1.0.14",
    "@rocicorp/zero": "0.7.2024120400",
    "@supabase/supabase-js": "^2.44.4",
    "@tamagui/button": "^1.120.1",
    "@tamagui/input": "^1.120.1",
    "@tamagui/linear-gradient": "^1.120.1",
    "@tamagui/lucide-icons": "^1.120.1",
    "@tamagui/proxy-tabbable": "^1.120.1",
    "@tamagui/react-native-web": "^1.120.1",
    "@tamagui/themes": "^1.120.1",
    "@tamagui/use-store": "^1.120.1",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-deep-link": "^2.0.1",
    "@tauri-apps/plugin-shell": "^2",
    "@vxrn/color-scheme": "1.1.340",
    "@vxrn/mdx": "1.1.340",
    "better-auth": "^1.0.10",
    "body-scroll-lock": "^4.0.0-beta.0",
    "mdx-bundler": "^10.0.2",
    "one": "1.1.340",
    "pg": "^8.13.1",
    "postmark": "^3.0.18",
    "react": "^18",
    "react-hotkeys-hook": "^4.6.1",
    "react-native": "0.74.1",
    "tamagui": "^1.120.1"
  },
  "devDependencies": {
    "@algolia/client-search": "^5.8.1",
    "@tauri-apps/cli": "^2",
    "gray-matter": "^4.0.3",
    "marked": "^14.1.2",
    "tsx": "^4.19.0",
    "vite-tsconfig-paths": "^5.0.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
