{
  "name": "test-test",
  "version": "1.1.349",
  "private": true,
  "type": "module",
  "installConfig": {
    "hoistingLimits": "workspaces"
  },
  "scripts": {
    "dev": "one dev",
    "prod": "one build && one serve",
    "dev:clean": "one dev --clean",
    "clean": "one clean",
    "build:web": "one build",
    "serve": "one serve",
    "orm:generate": "drizzle-kit generate --config ./db/drizzle.config.ts",
    "upgrade:tamagui": "$npm_execpath up '*tamagui*' '@tamagui/*'",
    "orm:push": "drizzle-kit push --config ./db/drizzle.config.ts",
    "orm:migrate": "dotenvx run -f .env -- tsx db/run-migrations.ts",
    "orm:seed": "dotenvx run -f .env -- tsx db/seed.ts"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.12.1",
    "@tamagui/config": "^1.120.2",
    "@tamagui/image-next": "^1.120.2",
    "@tamagui/lucide-icons": "^1.120.2",
    "@tamagui/vite-plugin": "^1.120.2",
    "@vercel/og": "^0.6.3",
    "@vxrn/color-scheme": "workspace:*",
    "better-sqlite3": "^11.2.1",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.24.2",
    "drizzle-orm": "^0.33.0",
    "expo": "~51.0.28",
    "one": "workspace:*",
    "postgres": "^3.4.4",
    "react": "^18.3.1",
    "react-native": "0.74.5",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-svg": "15.2.0",
    "react-native-web": "^0.19.12",
    "tamagui": "^1.120.2"
  },
  "devDependencies": {
    "@faker-js/faker": "next",
    "tsx": "^4.19.0",
    "vite": "^6.0.3"
  }
}
